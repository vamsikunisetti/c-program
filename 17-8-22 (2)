#include <stdio.h>

int main()
{
    int n,arr[10],i,sum=0,total;
    printf("enter the n value:\n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
    n++;
    total=n*(n+1)/2;
    printf("the missing number is %d",total-sum);
    
    

    return 0;
}
