#include<stdio.h>
void main()
{
	char s[20];
	int i,j,len=0;
	printf("Enter a string\n");
    scanf("%s",s);
	for(i=0;s[i]!=0;i++)
	{
	    len++;
	}
	i=0;
	j=len-1;
	while(i<j)
	{
	    if(s[i]!=s[j])
	    
	          break;
	    i++;
	    j--;
	}
	if(i>=j)
	printf("given string is palindrome\n");
	else
	printf("not a palidrome");
	
}
